function selectLobbyServer(host) {
  // Ø§Ø³ØªØ®Ø¯Ø§Ù… hash Ø¨Ø³ÙŠØ· Ù„ØªÙˆØ²ÙŠØ¹ Ø§Ù„Ø­Ù…Ù„
  var hash = 0;
