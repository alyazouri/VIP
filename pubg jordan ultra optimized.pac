// ØªØ­Ø¯ÙŠØ« ØµØ­Ø© Ø§Ù„Ø®Ø§Ø¯Ù… (Server Health)
// â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
function updateServerHealth(server, success, latency) {
  if (!SESSION.serverHealth[server]) {
    SESSION.serverHealth[server] = {
      failures: 0,
      successes: 0,
      totalLatency: 0,
      avgLatency: 0,
      healthy: true
    };
  }
  
  var health = SESSION.serverHealth[server];
  
  if (success) {
    health.successes++;
    if (latency) {
      health.totalLatency += latency;
      health.avgLatency = health.totalLatency / health.successes;
    }
    // ØªÙ‚Ù„ÙŠÙ„ Ø¹Ø¯Ø§Ø¯ Ø§Ù„ÙØ´Ù„ ØªØ¯Ø±ÙŠØ¬ÙŠØ§Ù‹
    health.failures = Math.max(0, health.failures - 1);
    if (health.failures < 2) {
      health.healthy = true;
    }
  } else {
    health.failures++;
    // Ø¥Ø°Ø§ ÙØ´Ù„ 3 Ù…Ø±Ø§Øª Ù…ØªØªØ§Ù„ÙŠØ©ØŒ Ø§Ø¹ØªØ¨Ø±Ù‡ ØºÙŠØ± ØµØ­ÙŠ
    if (health.failures >= 3) {
      health.healthy = false;
    }
  }
}

