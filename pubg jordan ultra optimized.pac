function isJordanIP(ip) {
  // ÙØ­Øµ Ø³Ø±ÙŠØ¹: Ù‡Ù„ Ø§Ù„Ø¨Ø¯Ø§ÙŠØ© ØªØ·Ø§Ø¨Ù‚ Ø£ÙŠ Ù†Ø·Ø§Ù‚ Ø£Ø±Ø¯Ù†ÙŠØŸ
  var firstTwo = ip.split('.').slice(0, 2).join('.');
  
  // Ù‚Ø§Ø¦Ù…Ø© Ø§Ù„Ø¨Ø¯Ø§ÙŠØ§Øª Ø§Ù„Ø´Ø§Ø¦Ø¹Ø© Ù„Ù„Ø³Ø±Ø¹Ø© (ØªØ¬Ù†Ø¨ loop ÙƒØ§Ù…Ù„)
  var commonPrefixes = ["46.185", "212.35", "176.28", "176.29", "82.212", "82.213"];
  for (var i = 0; i < commonPrefixes.length; i++) {
    if (firstTwo === commonPrefixes[i]) return true;
  }
  
  // ÙØ­Øµ ÙƒØ§Ù…Ù„ Ù„Ù„Ø¨Ù‚ÙŠØ©
  for (var i = 0; i < JORDAN_ISP_RANGES.length; i++) {
    var range = JORDAN_ISP_RANGES[i];
    if (isIPInRange(ip, range.network, range.mask)) {
      return true;
    }
  }
  
  return false;
}

