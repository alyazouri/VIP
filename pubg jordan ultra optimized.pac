function isIPInRange(ip, network, netmask) {
  // Ø§Ø³ØªØ®Ø¯Ø§Ù… Ø§Ù„Ø¯Ø§Ù„Ø© Ø§Ù„Ù…Ø¯Ù…Ø¬Ø© isInNet (Ø£Ø³Ø±Ø¹ Ù…Ù† Ø§Ù„Ø­Ø³Ø§Ø¨ Ø§Ù„ÙŠØ¯ÙˆÙŠ)
