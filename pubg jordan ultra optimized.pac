function periodicOptimization() {
  var now = Date.now();
  
  // Ù†ÙØ° Ù…Ø±Ø© ÙƒÙ„ Ø¯Ù‚ÙŠÙ‚Ø© ÙÙ‚Ø·
  if (now - SESSION.lastOptimization < 60000) return;
  
  SESSION.lastOptimization = now;
  
  // ØªÙ†Ø¸ÙŠÙ DNS cache Ø§Ù„Ù‚Ø¯ÙŠÙ…
  for (var host in SESSION.dns) {
    if (now - SESSION.dns[host].time > CONFIG.DNS_CACHE_TIME * 3) {
      delete SESSION.dns[host];
    }
  }
  
  // Ø¥Ø¹Ø§Ø¯Ø© ØªÙ‚ÙŠÙŠÙ… ØµØ­Ø© Ø§Ù„Ø®ÙˆØ§Ø¯Ù…
  for (var server in SESSION.serverHealth) {
    var health = SESSION.serverHealth[server];
    // Ø¥Ø°Ø§ Ù„Ù… ÙŠØ³ØªØ®Ø¯Ù… Ø§Ù„Ø®Ø§Ø¯Ù… Ù„ÙØªØ±Ø© Ø·ÙˆÙŠÙ„Ø©ØŒ Ø£Ø¹Ø¯ ØªØ¹ÙŠÙŠÙ† Ø§Ù„Ø¥Ø­ØµØ§Ø¦ÙŠØ§Øª
