// ÙƒØ´Ù Ù…Ø²ÙˆØ¯ Ø§Ù„Ø®Ø¯Ù…Ø© (ISP) Ù…Ù† Ø¹Ù†ÙˆØ§Ù† IP
// â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
function detectISP(ip) {
  // Ø§Ù„Ø¨Ø­Ø« ÙÙŠ Ù‚Ø§Ø¹Ø¯Ø© Ø§Ù„Ø¨ÙŠØ§Ù†Ø§Øª Ø¹Ù† Ø§Ù„Ù…Ø²ÙˆØ¯ Ø§Ù„Ù…Ø·Ø§Ø¨Ù‚
  for (var i = 0; i < JORDAN_ISP_RANGES.length; i++) {
    var range = JORDAN_ISP_RANGES[i];
    if (isIPInRange(ip, range.network, range.mask)) {
      
      // Ø¥Ø¶Ø§ÙØ© Ø¹ÙŠÙ†Ø© Ù„Ù„ØªØ­Ù„ÙŠÙ„ Ø§Ù„Ø¥Ø­ØµØ§Ø¦ÙŠ
      SESSION.ispSamples.push(range.isp);
      if (SESSION.ispSamples.length > 20) {
        SESSION.ispSamples.shift(); // Ø¥Ø²Ø§Ù„Ø© Ø§Ù„Ø£Ù‚Ø¯Ù…
      }
      
      // Ø­Ø³Ø§Ø¨ Ù…Ø¯Ù‰ Ø§Ù„Ø«Ù‚Ø© Ø¨Ø§Ù„ÙƒØ´Ù
      var counts = {};
      for (var j = 0; j < SESSION.ispSamples.length; j++) {
        var isp = SESSION.ispSamples[j];
        counts[isp] = (counts[isp] || 0) + 1;
      }
      
      var maxCount = 0;
      var dominantISP = null;
      for (var isp in counts) {
        if (counts[isp] > maxCount) {
          maxCount = counts[isp];
          dominantISP = isp;
        }
      }
      
      SESSION.detectedISP = dominantISP;
      SESSION.ispConfidence = Math.round((maxCount / SESSION.ispSamples.length) * 100);
      
      return range;
    }
  }
  
  return null;
}

